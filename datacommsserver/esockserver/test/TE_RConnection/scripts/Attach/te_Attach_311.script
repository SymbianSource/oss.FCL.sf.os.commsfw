RUN_TEST_STEP 100 te_esockteststepsSuite creatersocketStep z:\testdata\configs\Attach\te_Attach_311.ini CreateUdpSock1
RUN_TEST_STEP 100 te_esockteststepsSuite connectrsocketservStep z:\testdata\configs\Attach\te_Attach_311.ini ConnectSockSvr1
RUN_TEST_STEP 100 te_esockteststepsSuite openrconnectionStep z:\testdata\configs\Attach\te_Attach_311.ini OpenConn1
