//! @file contentionmanagement_001.script
//! @SYMTestSuiteName COMINF-ESOCK-RConnection-ContentionManagement-0106
//! @SYMScriptTestEnvironment  networking

// Load the test suite and esock
LOAD_SUITE te_esockteststepsSuite -SharedData


RUN_SCRIPT z:\TestData\scripts\te_esock_test_loadesock.script

START_TESTCASE			COMINF-ESOCK-RConnection-contention-0101
//! @SYMTestCaseID		COMINF-ESOCK-RConnection-contention-0101
//!@SYMPREQ PREQ1927
//!@SYMTestCaseDesc Start two connections Conn1 and Conn2 one by one having only one primary PDP context available. Conn2 is more prioritized, as a result Conn1. Should be stopped and Conn2 should be started without an error
//!@SYMTestPriority Critical
//!@SYMTestActions Start two connections Conn1 and Conn2 one by one having only one primary PDP context available. Conn2 is more prioritized, as a result Conn1. Should be stopped and Conn2 should be started without an error
//!@SYMTestExpectedResults Pass
//!@SYMTestType CIT
//! @SYMCreationDate        28-11-2008
//!@SYMTestStatus 3. Released

RUN_TEST_STEP 100 te_esockteststepsSuite creatersocketservStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini CreateSockSvr1
RUN_TEST_STEP 100 te_esockteststepsSuite createrconnectionStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini CreateConn1
RUN_TEST_STEP 100 te_esockteststepsSuite createrconnectionStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini CreateConn2
RUN_TEST_STEP 100 te_esockteststepsSuite creatersocketStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini CreateUdpSock1
RUN_TEST_STEP 100 te_esockteststepsSuite creatersocketStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini CreateUdpSock2


RUN_TEST_STEP 100 te_esockteststepsSuite connectrsocketservStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini ConnectSockSvr1

RUN_TEST_STEP 100 te_esockteststepsSuite openrconnectionStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini OpenConn1
RUN_TEST_STEP 100 te_esockteststepsSuite startrconnectionStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini StartConn1
RUN_TEST_STEP 100 te_esockteststepsSuite openrsocketStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini OpenUdpSock1
RUN_TEST_STEP 100 te_esockteststepsSuite connectrsocketStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini ConnectUdpSock1


RUN_TEST_STEP 100 te_esockteststepsSuite openrconnectionStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini OpenConn2
RUN_TEST_STEP 100 te_esockteststepsSuite startrconnectionStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini StartConn2
RUN_TEST_STEP 100 te_esockteststepsSuite openrsocketStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini OpenUdpSock2
RUN_TEST_STEP 100 te_esockteststepsSuite connectrsocketStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini ConnectUdpSock2


RUN_TEST_STEP 100 te_esockteststepsSuite closersocketStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini CloseUdpSock1
RUN_TEST_STEP 100 te_esockteststepsSuite closerconnectionStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini CloseConn1
RUN_TEST_STEP 100 te_esockteststepsSuite closerconnectionStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini CloseConn2
RUN_TEST_STEP 100 te_esockteststepsSuite closersocketservStep z:\testdata\configs\ContentionManagement\ContentionManagement_001.ini CloseSockSvr1

RUN_TEST_STEP 100 te_esockteststepsSuite cleanallStep

RUN_SCRIPT Z:\TestData\scripts\te_esock_test_unloadesockForced.script

END_TESTCASE			COMINF-ESOCK-RConnection-contention-0101

PRINT Completed COMINF-ESOCK-RConnection-contention-0101



