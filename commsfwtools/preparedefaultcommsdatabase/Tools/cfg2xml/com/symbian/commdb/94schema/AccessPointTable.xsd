<xsd:schema elementFormDefault="qualified" targetNamespace="SymbianOS.Generic.Comms-Infras.CommDB"
      xmlns="SymbianOS.Generic.Comms-Infras.CommDB" xmlns:xlink="http://www.w3.org/1999/xlink" 
      xmlns:xsd="http://www.w3.org/2001/XMLSchema">
      <!--
          Include Symbian primitives and Comms elements
      -->
      <xsd:include schemaLocation="PrimitiveTypes.xsd"/>
      <xsd:include schemaLocation="CommTypes.xsd"/>
      <xsd:import namespace="http://www.w3.org/1999/xlink" schemaLocation="XLinkAttributes.xsd"/>
      <!-- 
          Definition of the AccessPointTable element
      -->
      <xsd:element name="AccessPointTable">
            <xsd:complexType>
                  <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="AccessPoint"/>
                  </xsd:sequence>
                  <xsd:attribute ref="xlink:type"/>
            </xsd:complexType>
      </xsd:element>
      <xsd:element name="AccessPoint">
            <xsd:complexType>
                  <xsd:all>
                        <xsd:element maxOccurs="1" minOccurs="0" name="Hidden" type="TBool"/>
                        <xsd:element maxOccurs="1" minOccurs="0" name="ReadOnly" type="TBool"/>
                        <xsd:element maxOccurs="1" minOccurs="0" name="Id" type="TUint32"/>
                        <xsd:element maxOccurs="1" minOccurs="1" name="Name" type="Text"/>
                        <xsd:element maxOccurs="1" minOccurs="0" name="Tier" type="TUint32"/>
                        <xsd:element maxOccurs="1" minOccurs="0" name="MCpr" type="TUint32"/>
                        <xsd:element maxOccurs="1" minOccurs="0" name="Cpr" type="TUint32"/>
                        <xsd:element maxOccurs="1" minOccurs="0" name="CprConfig" type="TUint32"/>
                        <xsd:element maxOccurs="1" minOccurs="0" name="SCpr" type="TUint32"/>
                        <xsd:element maxOccurs="1" minOccurs="0" name="Protocol" type="TUint32"/>
                        <xsd:element maxOccurs="1" minOccurs="0" name="AppSID" type="TUint32"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="ConfigAPIdList" type="Text" />
                  </xsd:all>
                  <xsd:attribute ref="xlink:title"/>
                  <xsd:attribute ref="xlink:role"/>
                  <xsd:attribute ref="xlink:type"/>
                  <xsd:attribute name="operation" type="OperationType"/>
            </xsd:complexType>
      </xsd:element>
</xsd:schema>
